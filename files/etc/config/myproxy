config device_proxy 'example'
    option ip '192.168.1.100'
    option proxy_ip '127.0.0.1'
    option proxy_port '1080'
    option proxy_type 'socks5'
    option enabled '1'

config device_proxy 'example_http'
    option ip '192.168.1.101'
    option proxy_ip '192.168.1.200'
    option proxy_port '8080'
    option proxy_type 'http'
    option enabled '1'